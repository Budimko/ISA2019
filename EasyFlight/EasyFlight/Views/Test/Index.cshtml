@model EasyFlight.ViewModel.SeatViewModel

<form method="post">
    <div class="form-group">
        <label for="login-email">RowNumber</label>
        <input type="text" class="form-control" name="RowNumber" placeholder="RowNumber">
    </div>
    <div class="form-group">
        <label for="login-password">ColumnNumber</label>
        <input type="text" class="form-control" name="ColumnNumber" placeholder="ColumnNumber">
    </div>
    <button type="submit" class="btn btn-success">Submit</button>
</form>